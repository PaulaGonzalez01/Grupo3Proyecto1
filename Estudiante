package proyecto_poo;

/**
 *
 * @author E6530
 */
public class Estudiante extends Persona{
    public int opcion;

    public Estudiante(int opcion, String nombre, String apellido, int edad, String casa, String varita) {
        super(nombre, apellido, edad, casa, varita);
        this.opcion = opcion;
    }
    
    
}
